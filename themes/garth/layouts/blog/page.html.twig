{% extends 'page.html.twig' %}

{% block content %}
        <article class="article article--page typeset">
          <h1>{{ page.title }}</h1>
          {%~ include 'partials/post-meta.html.twig' with {'post': page} only ~%}
          {% if page.image is defined %}<img src="{{ asset(page.image)|url }}" alt="{{ page.title }}" />{% endif %}
          {{ page.content }}
        </article>
{% endblock content %}
