{
  "name": "cecil/theme-installer",
  "description": "Installer for Cecil's themes",
  "type": "composer-plugin",
  "keywords": [
    "cecil",
    "installer",
    "themes"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Arnaud Ligny",
      "email": "arnaud@ligny.fr",
      "homepage": "https://arnaudligny.fr",
      "role": "Developer"
    }
  ],
  "require": {
    "composer-plugin-api": ">=1.0"
  },
  "require-dev": {
    "composer/composer": ">=1.0"
  },
  "autoload": {
    "psr-0": {
      "Cecil\\Installer\\Composer": "src/"
    }
  },
  "extra": {
    "class": "Cecil\\Installer\\Composer\\CecilPlugin",
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
